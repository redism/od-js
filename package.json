{
  "name": "od-js",
  "version": "0.2.0",
  "description": "Opiniated js utility belt by overdosed.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "babel src -d lib",
    "build:watch": "babel src -d lib -w",
    "lint": "eslint 'src/**/*.js' 'tests/**/*.js'",
